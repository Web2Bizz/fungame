{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["ES2022"],
    "module": "NodeNext",
    "rootDir": "./src",
    "baseUrl": "./src",
    "declaration": false,
    "removeComments": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
